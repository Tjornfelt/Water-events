  <FrontPage id="1050" parentID="-1" level="1" creatorID="0" sortOrder="0" createDate="2015-01-24T11:59:25" updateDate="2015-02-07T15:24:55" nodeName="Forside" urlName="forside" path="-1,1050" isDoc="" nodeType="1047" creatorName="Mads Tjørnfelt" writerName="Michael" writerID="1" template="1049" nodeTypeAlias="FrontPage">
